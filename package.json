{
  "name": "min-calc",
  "version": "1.0.1",
  "description": "Mini calculator",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "release": "electron-builder --publish always"
  },
  "repository": "https://github.com/impugachev/electron-calc",
  "keywords": [
    "Electron",
    "calculator"
  ],
  "author": "impugachev",
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^22.3.2"
  },
  "dependencies": {
    "about-window": "^1.13.2",
    "jquery": "^3.5.0",
    "lodash": "^4.17.10"
  },
  "build": {
    "appId": "min-calc",
    "win": {
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publish": ["github"]
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64"
          ]
        }
      ],
      "publish": ["github"]
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "publish": ["github"]
    }
  }
}
